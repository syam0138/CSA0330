#include <stdio.h>
int main() 
{
    int a[50], n, i, low, high, mid, key;
    printf("Enter n: ");
    scanf("%d",&n);

    for(i=0;i<n;i++)
        scanf("%d",&a[i]);

    printf("Enter key: ");
    scanf("%d",&key);

    low=0; high=n-1;
    while(low<=high) {
        mid=(low+high)/2;
        if(a[mid]==key) {
            printf("Found");
            return 0;
        }
        else if(a[mid]<key)
            low=mid+1;
        else
            high=mid-1;
    }
    printf("Not found");
    return 0;
}
