#include <stdio.h>
int a[10][10], v[10]={0}, n;
void dfs(int s)
{
    int i;
    printf("%d ",s);
    v[s]=1;
    for(i=0;i<n;i++)
        if(a[s][i] && !v[i])
            dfs(i);
}

int main(){
    int i,j,s;
    printf("Enter n: ");
    scanf("%d",&n);
    for(i=0;i<n;i++)
        for(j=0;j<n;j++)
            scanf("%d",&a[i][j]);
    printf("Enter start: ");
    scanf("%d",&s);
    dfs(s);
    return 0;
}
