#include <stdio.h>
#include <string.h>
char stack[20];
int top=-1;
void push(char c){ stack[++top]=c; }
char pop(){ return stack[top--]; }

int main() 
{
    char exp[20];
    int i;
    printf("Enter expression: ");
    scanf("%s",exp);

    for(i=0;i<strlen(exp);i++) {
        if(exp[i]>='a'&&exp[i]<='z')
            printf("%c",exp[i]);
        else {
            push(exp[i]);
        }
    }
    while(top!=-1)
        printf("%c",pop());
    return 0;
}
