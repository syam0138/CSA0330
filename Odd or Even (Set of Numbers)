#include <stdio.h>
int main() 
{
    int n, a[50], i;
    printf("Enter number of elements: ");
    scanf("%d",&n);

    for(i=0;i<n;i++)
        scanf("%d",&a[i]);

    for(i=0;i<n;i++) {
        if(a[i]%2==0)
            printf("%d is Even\n",a[i]);
        else
            printf("%d is Odd\n",a[i]);
    }
    return 0;
}
